extends layout

block kat
    script(src='/javascripts/script.js')
    h1= title
    p Welcome to #{title} #{message}!
    form(name="registerForm")
        label Name
        br
        input(type="text", name="userName")
        br
        label Age
        br
        input(type="number", name="userAge")
        br
        button#submit(type="submit") Send


